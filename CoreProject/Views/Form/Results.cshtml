@model Product

@{
    ViewBag.Title = "Results";
}

<h2 class="bg-primary text-white text-center p-2">Results</h2>

<table class="table table-bordered table-striped">
    <tr>
        <th colspan="2">Form Data</th>
    </tr>
    @foreach (string key in TempData.Keys)
    {
        <tr>
            <td>@key</td>
            <td>@TempData[key]</td>
        </tr>
    }
   
</table>

<hr />

<a asp-action="Index">Go back</a>